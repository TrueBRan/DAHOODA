Godfull.Font = Enum.Font.GothamBlack
Godfull.Text = "Godfull"
Godfull.TextColor3 = Color3.fromRGB(255, 255, 255)
Godfull.TextSize = 14.000
Godfull.MouseButton1Down:connect(function()
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://413861777"
	sound.Parent = game:GetService("SoundService")
	sound:Play()

